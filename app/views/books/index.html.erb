<h1>Daftar Buku</h1>
<table>
  <thead>
    <tr>
      <th>Judul</th>
      <th>Pengarang</th>
      <th>Tahun Terbit</th>
    </tr>
  </thead>
  <tbody>
    <% @books.each do |book| %>
      <tr>
        <td><%= book.title %></td>
        <td><%= book.author %></td>
      </tr>
    <% end %>
  </tbody>
</table>
