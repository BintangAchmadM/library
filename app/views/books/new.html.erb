<%= form_with(model: @book, local: true) do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
  
    <%= form.label :year %>
    <%= form.number_field :year %>
  
    <%= form.label :author_id %>
    <%= form.collection_select :author_id, Author.all, :id, :username %>
  
    <%= form.submit 'Create Book' %>
  <% end %>
  